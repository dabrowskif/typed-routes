{
  "name": "@dabrowskif/typed-routes",
  "version": "0.5.0",
  "license": "MIT",
  "author": "dabrowskif",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "tsc && node dist/index.js -r test/sveltekit-tests/test1/routes -f sveltekit",
    "test:e2e:verbose": "tsc && node dist/index.js -r test/sveltekit-tests/test1/routes -f sveltekit -v true",
    "test:e2e:watch": "nodemon --exec npm run test:e2e --watch test/sveltekit-tests/test1/routes"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dabrowskif/typed-routes.git"
  },
  "bin": {
    "typed-routes": "dist/index.js"
  },
  "dependencies": {
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@swc/core": "^1.7.18",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.7",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
