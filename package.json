{
  "name": "@dabrowskif/typed-routes",
  "version": "0.5.0",
  "license": "MIT",
  "author": "dabrowskif",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test:watch": "jest --watch",
    "dev": "npm run build && npm run start",
    "test": "jest",
    "test:e2e": "tsc && node dist/index.js -r test/sveltekit-tests/test1/routes -f sveltekit",
    "test:e2e:verbose": "tsc && node dist/index.js -r test/sveltekit-tests/test1/routes -f sveltekit -v true",
    "test:e2e:watch": "nodemon --exec npm run test:e2e --watch test/sveltekit-tests/test1/routes",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dabrowskif/typed-routes.git"
  },
  "bin": {
    "typed-routes": "dist/index.js"
  },
  "dependencies": {
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^8.4.1",
    "@swc/core": "^1.7.18",
    "@swc/jest": "^0.2.36",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.7",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  }
}
