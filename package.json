{
  "name": "@dabrowskif/typed-routes",
  "descripton": "Fully typed routes generator for frontend meta frameworks.",
  "version": "0.6.1",
  "license": "MIT",
  "author": "dabrowskif",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "husky",
    "lint:ci": "eslint lib",
    "format:ci": "prettier lib --check",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "tsc && node dist/index.js -r test/sveltekit-tests/test1/routes -f sveltekit",
    "test:e2e:verbose": "tsc && node dist/index.js -r test/sveltekit-tests/test1/routes -f sveltekit -v true",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dabrowskif/typed-routes.git"
  },
  "keywords": [
    "typescript",
    "routes",
    "typed-routes",
    "javascript",
    "routing",
    "sveltekit",
    "nextjs"
  ],
  "bin": {
    "typed-routes": "index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^8.4.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.18",
    "@swc/jest": "^0.2.36",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.7",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  }
}
